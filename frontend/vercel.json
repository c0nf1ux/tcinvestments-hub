{
 "version": 2,
 "name": "brainstorm-frontend",
 "builds": [
   {
     "src": "package.json",
     "use": "@vercel/static-build",
     "config": {
       "distDir": "build"
     }
   }
 ],
 "routes": [
   {
     "src": "/static/(.*)",
     "dest": "/static/$1"
   },
   {
     "src": "/(.*)",
     "dest": "/index.html"
   }
 ],
 "env": {
   "REACT_APP_API_URL": "https://brainstorm-backend.railway.app",
   "REACT_APP_ENVIRONMENT": "production"
 }
}